{
  "name": "vbb-map-routing",
  "description": "Use VBB interactively, using a map.",
  "version": "0.1.0",
  "keywords": [
    "vbb",
    "map",
    "bvg",
    "routing"
  ],
  "author": "Jannis R <mail@jannisr.de>",
  "homepage": "http://derhuerst.github.io/vbb-map-routing/",
  "repository": "derhuerst/vbb-map-routing",
  "bugs": "https://github.com/derhuerst/vbb-map-routing/issues",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "es2020": "^1.1.9",
    "stringify": "^5.1.0",
    "uglify-js": "mishoo/UglifyJS2#harmony",
    "wzrd": "^1.5.0"
  },
  "scripts": {
    "bundle": "browserify -t es2020 index.js > bundle.js",
    "minify": "cat bundle.js | uglifyjs -mc > bundle.min.js",
    "build": "npm run bundle && npm run minify",
    "start": "wzrd index.js:bundle.js -- -t es2020"
  },
  "dependencies": {
    "@turf/line-slice": "^3.7.0",
    "abs-svg-path": "^0.1.1",
    "bvg-topological-map": "derhuerst/bvg-topological-map",
    "debounce": "^1.0.0",
    "global": "^4.3.1",
    "ms": "^0.7.2",
    "parse-svg-path": "^0.1.2",
    "simplify-geometry": "0.0.2",
    "svg-path-properties": "^0.1.1",
    "vbb-client": "^1.0.0",
    "vhtml": "^2.1.0",
    "virtual-dom": "^2.1.1"
  }
}
