{
  "name": "vbb-map-routing",
  "description": "Use VBB interactively, using a map.",
  "version": "0.1.0",
  "keywords": [
    "vbb",
    "map",
    "bvg",
    "routing"
  ],
  "author": "Jannis R <mail@jannisr.de>",
  "homepage": "http://derhuerst.github.io/vbb-map-routing/",
  "repository": "derhuerst/vbb-map-routing",
  "bugs": "https://github.com/derhuerst/vbb-map-routing/issues",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "es2020": "^1.1.9",
    "stringify": "^5.1.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "bundle": "browserify -t [ stringify --extensions [.svg] ] -t es2020 index.js > bundle.js",
    "minify": "cat bundle.js | uglifyjs -mc > bundle.min.js",
    "build": "npm run bundle && npm run minify",
    "start": "watchify -t [ stringify --extensions [.svg] ] -t es2020 index.js -o bundle.js"
  },
  "dependencies": {
    "global": "^4.3.1"
  }
}
