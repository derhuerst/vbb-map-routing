{
  "name": "vbb-map-routing",
  "description": "Use VBB interactively, using a map.",
  "version": "0.1.0",
  "keywords": [
    "vbb",
    "map",
    "bvg",
    "routing"
  ],
  "author": "Jannis R <mail@jannisr.de>",
  "homepage": "http://derhuerst.github.io/vbb-map-routing/",
  "repository": "derhuerst/vbb-map-routing",
  "bugs": "https://github.com/derhuerst/vbb-map-routing/issues",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^15.0.0",
    "stringify": "^5.1.0",
    "uglify-es": "^3.3.7",
    "wzrd": "^1.5.0"
  },
  "scripts": {
    "bundle": "browserify -t [babelify --presets env] index.js > dist/bundle.js",
    "minify": "cat dist/bundle.js | uglifyjs -mc > dist/bundle.min.js",
    "build": "npm run bundle && npm run minify",
    "start": "cd dist; wzrd ../index.js:bundle.js -- -t es2020"
  },
  "dependencies": {
    "@turf/line-slice": "^5.0.4",
    "abs-svg-path": "^0.1.1",
    "bvg-topological-map": "^0.1.1",
    "csjs": "^1.0.6",
    "debounce": "^1.0.0",
    "dom-delegator": "^13.1.0",
    "global": "^4.3.1",
    "insert-styles": "^1.2.1",
    "ms": "^2.0.0",
    "parse-svg-path": "^0.1.2",
    "scroll-into-view": "^1.7.4",
    "simplify-geometry": "0.0.2",
    "svg-path-properties": "^0.4.0",
    "vbb-client": "^2.0.0",
    "vbb-journey-ui": "github:derhuerst/vbb-journey-ui",
    "vbb-stations": "^5.2.0",
    "vbb-util": "^0.10.1",
    "vhtml": "^2.1.0",
    "virtual-dom": "^2.1.1"
  }
}
