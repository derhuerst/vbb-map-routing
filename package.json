{
  "name": "vbb-map-routing",
  "description": "Use VBB interactively, using a map.",
  "version": "0.1.0",
  "keywords": [
    "vbb",
    "map",
    "bvg",
    "routing"
  ],
  "author": "Jannis R <mail@jannisr.de>",
  "homepage": "http://derhuerst.github.io/vbb-map-routing/",
  "repository": "derhuerst/vbb-map-routing",
  "bugs": "https://github.com/derhuerst/vbb-map-routing/issues",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^10.0.0",
    "browserify": "^16.0.0",
    "envify": "^4.1.0",
    "stringify": "^5.1.0",
    "uglify-es": "^3.3.9",
    "uglifyify": "^5.0.0",
    "wzrd": "^1.5.0"
  },
  "scripts": {
    "bundle": "env NODE_ENV=production browserify -g [babelify --presets env --ignore '/node_modules\\/virtual-dom/'] -g envify -g uglifyify index.js | uglifyjs -mc > dist/bundle.js",
    "css": "cp $(node -e \"process.stdout.write(require.resolve('vbb-journey-ui/styles.css'))\") dist/vbb-journey-ui.css",
    "build": "npm run bundle && npm run css",
    "build:dev": "env NODE_ENV=dev browserify --debug -g envify index.js > dist/bundle.js",
    "start": "cd dist; env NODE_ENV=dev wzrd ../index.js:bundle.js -- -g envify"
  },
  "dependencies": {
    "@turf/line-slice": "^5.0.4",
    "abs-svg-path": "^0.1.1",
    "bvg-topological-map": "^0.2.0",
    "csjs": "^1.0.6",
    "debounce": "^1.0.0",
    "dom-delegator": "github:derhuerst/dom-delegator",
    "global": "^4.3.1",
    "insert-styles": "^1.2.1",
    "luxon": "^1.2.0",
    "ms": "^2.1.1",
    "parse-svg-path": "^0.1.2",
    "scroll-into-view": "^1.7.4",
    "simplify-geometry": "0.0.2",
    "svg-path-properties": "^0.4.0",
    "vbb-client": "^3.0.1",
    "vbb-journey-ui": "^2.0.3",
    "vbb-stations": "^6.0.0",
    "vbb-util": "^0.10.1",
    "vhtml": "^2.1.0",
    "virtual-dom": "^2.1.1"
  }
}
