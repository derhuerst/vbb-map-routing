    "csjs": "^1.0.6",
    "debounce": "^1.0.0",
    "dom-delegator": "^13.1.0",
    "global": "^4.3.1",
    "insert-styles": "^1.2.1",
    "ms": "^2.0.0",
    "parse-svg-path": "^0.1.2",
    "scroll-into-view": "^1.7.4",
    "simplify-geometry": "0.0.2",
    "svg-path-properties": "^0.1.1",
    "vbb-client": "^2.0.0",
    "vbb-util": "^0.10.1",
    "vbb-stations": "^5.2.0",
    "vhtml": "^2.1.0",
    "virtual-dom": "^2.1.1"
  }
}
